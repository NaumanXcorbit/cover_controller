<h1>Clients#index</h1>
  <table>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Status</th>
    </tr>
    <tbody>
    <% @clients.each do |i| %>
    <tr>
      <td><%= i.name %></td>
      <td><%= i.email %></td>
      <td><%= i.status %></td>
    </tr>
    <% end %>
    </tbody>
  </table>